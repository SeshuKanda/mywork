      "source": [
        "print(\"IDF of: \")\n",
        "\n",
        "idf = {}\n",
        "\n",
        "for w in words_set:\n",
        "  k = 0 # number of documents in the corpus that contain this word\n",
        "\n",
        "  for i in range(n_docs):\n",
        "    if w in corpus[i].split():\n",
        "       k += 1\n",
        "\n",
        "  idf[w] = np.log10(n_docs / k)\n",
        "  print(f'{w:>15}: {idf[w]:>10}' )"
      ],
